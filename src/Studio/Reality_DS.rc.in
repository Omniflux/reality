#ifdef RC_INVOKED

#include <winresrc.h>
#include "build_number.h"

#define STRINGIZE_(arg) #arg
#define STRINGIZE(arg) STRINGIZE_(arg)

LANGUAGE LANG_ENGLISH, SUBLANG_ENGLISH_US

VS_VERSION_INFO VERSIONINFO
 FILEVERSION @Reality_VERSION_MAJOR@, @Reality_VERSION_MINOR@, @Reality_VERSION_PATCH@, BUILD_NUMBER
 PRODUCTVERSION @Reality_VERSION_MAJOR@, @Reality_VERSION_MINOR@, @Reality_VERSION_PATCH@, @Reality_VERSION_TWEAK@
 FILEFLAGSMASK VS_FFI_FILEFLAGSMASK
#ifdef _DEBUG
 FILEFLAGS VS_FF_DEBUG
#else
 FILEFLAGS 0x0L
#endif
FILEOS VOS_NT
FILETYPE VFT_DLL
FILESUBTYPE VFT2_UNKNOWN

BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName", STRINGIZE(Pret-a-3D\0)
            VALUE "FileDescription", STRINGIZE(DAZ Studio Reality Plug-in\0)
            VALUE "FileVersion", STRINGIZE(@Reality_VERSION_MAJOR@.@Reality_VERSION_MINOR@.@Reality_VERSION_PATCH@.BUILD_NUMBER\0)
            VALUE "InternalName", STRINGIZE(Reality_DS.dll\0)
            VALUE "LegalCopyright", STRINGIZE(Pret-a-3D/Paolo Ciccone\0)
            VALUE "OriginalFilename", STRINGIZE(Reality_DS.dll\0)
            VALUE "ProductName", STRINGIZE(@Reality_DESCRIPTION@\0)
            VALUE "ProductVersion", STRINGIZE(@Reality_VERSION_MAJOR@.@Reality_VERSION_MINOR@.@Reality_VERSION_PATCH@.@Reality_VERSION_TWEAK@\0)
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END

#endif
